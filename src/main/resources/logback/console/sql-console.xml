<?xml version="1.0" encoding="UTF-8"?>
<included>
    <appender name="SQL_CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{yyyy-MM-dd HH:mm:ss.SSS} %clr(%-5level){blue} %clr([%X{request_id}]){magenta} %clr(%logger{30}){cyan} - %msg%n</pattern>
        </encoder>
    </appender>
</included>
